#!/usr/bin/groovy
@Library('pipeline-library-sqli') _
pipeline {
    agent any
    tools {
        maven 'maven-3-5-3'
    }
    stages {
        stage('Checkout Git') {
          steps {
            script { 
                gitlabCheckout('${branch}','${url}')
             }
          }
        }
        stage('Build & Install & Test') {
            steps {
              script {
                 mavenInstall()
             }
            }
        }
        stage('Quality') {
            steps {
                script {
                 sonarQuality('SONAR')
             }
            }
        }

    }
}
